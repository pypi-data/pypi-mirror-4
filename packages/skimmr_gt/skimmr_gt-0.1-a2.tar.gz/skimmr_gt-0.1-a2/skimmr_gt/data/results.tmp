  <center>
    <!-- INPUT FORM -->
    <form method="POST" action="%s">
      <!-- NAME OF THE USER WHO GENERATED THE PARTICULAR PAGE -->
      <input type="hidden" name="usrn" value="%s"/>
      <!-- ACTION IMPLIED BY THE FORM ACTIVATION -->
      <input type="hidden" name="action" value="query"/>
      <!-- SPECIFIC FORM CONTENT FOLLOWS -->
      Ask a query: <input type="text" name="usrq"/>,
      follow a link:
      <select name="defq">
        <option value="None">== NONE SELECTED ==</option>
        <!-- LIST OF DYNAMICALLY GENERATED OPTIONS HERE -->
%s
      </select>,
      or go back in history:
      <select name="hist">
        <!-- LIST OF DYNAMICALLY GENERATED OPTIONS HERE -->
        <option value="None">== NONE SELECTED ==</option>
%s
      </select>
      <br/>
      <input type="submit" value="GO!"/>
      <br/>
      <br/>
      <small>
        <i>
          Not enough results? Try to increase the number of nodes and edges
          visible and ask again!
          <br/>
          Max. nodes:
          <!-- LIST OF DYNAMICALLY GENERATED OPTIONS HERE -->
%s
          Max. edges:
          <!-- LIST OF DYNAMICALLY GENERATED OPTIONS HERE -->
%s
        </i>
      </small>
    </form>
    <!-- VISUAL BREAK -->
    <br/>
    <br/>
    <hr/>
    <!-- RESULT DISPLAY (QUERY ENTITY NAME FILLED IN DYNAMICALLY) -->
    <h2>Entities linked to: <i>%s</i></h2>
    <iframe width="%s" height="%s" frameborder="1" scrolling="yes" 
    src="/data/usrs/%s/ent_net.htm">
      Entity network could not be displayed - the iframe HTML feature
      not supported by your browser.
    </iframe>
    <br/><br/>

    <!-- FRAME WITH EXTERNAL RESOURCES TO BE PUT HERE -->
%s

    <!-- GOOGLE ENTITY LOOK UP FORM -->
    <form method="GET" action="http://www.google.com/" target="_blank">
      <table class="lookup_left">
        <!-- SPECIFIC FORM CONTENT FOLLOWS -->
        <tr align="center"><td><h3>Entity look-up</h3></td></tr>
        <tr align="center"><td>
        Search <a href="http://www.google.com">Google</a> for the entity: 
        <select name="q">
          <option value="None">== NONE SELECTED ==</option>
          <!-- LIST OF DYNAMICALLY GENERATED OPTIONS HERE -->
%s
        </select>
        </td></tr>
        <tr align="center"><td><input type="submit" value="Proceed"/></td></tr>
      </table>
    </form>

    <!-- SOURCE LOOK UP FORM -->
    <form method="POST" action="%s">
      <table class="lookup_right">
      <!-- NAME OF THE USER WHO GENERATED THE PARTICULAR PAGE -->
      <input type="hidden" name="usrn" value="%s"/>
      <!-- CURRENT QUERY AND GRAPH SIZE PARAMETERS -->
      <input type="hidden" name="usrq" value="%s"/>
      <input type="hidden" name="maxn" value="%s"/>
      <input type="hidden" name="maxe" value="%s"/>
      <!-- ACTION IMPLIED BY THE FORM ACTIVATION -->
      <input type="hidden" name="action" value="source_lookup"/>
        <!-- SPECIFIC FORM CONTENT FOLLOWS -->
        <tr align="center"><td><h3>Related sources</h3></td></tr>
        <tr align="center"><td>
        Explore 
        <select name="maxa">
          <option value="10" selected="selected">10</option>
          <option value="20">20</option>
          <option value="50">50</option>
          <option value="100">100</option>
          <option value="200">200</option>
        </select>
        paragraphs most relevant to the query
        </td></tr>
        <tr align="center"><td><input type="submit" value="Refresh"/></td></tr>      </table>
    </form>

    <!-- VISUAL BREAK -->
    <br/>
    <br/>
    <br/>
    <br/>
    <br/>
    <hr/>
    <!-- FEEDBACK COLLECTION FORM -->
    <form method="POST" action="%s">
      <!-- NAME OF THE USER WHO GENERATED THE PARTICULAR PAGE -->
      <input type="hidden" name="usrn" value="%s"/>
      <!-- CURRENT QUERY AND GRAPH SIZE PARAMETERS -->
      <input type="hidden" name="usrq" value="%s"/>
      <input type="hidden" name="maxn" value="%s"/>
      <input type="hidden" name="maxe" value="%s"/>
      <!-- ACTION IMPLIED BY THE FORM ACTIVATION -->
      <input type="hidden" name="action" value="feedback"/>
      <!-- SPECIFIC FORM CONTENT FOLLOWS -->
      <h3>Feedback on the results (optional)</h3>
      <small>
      <table width="%s" border="1">
      <tr>
        <td align="center"><b>Entity network</b></td>
        <td align="center"><b>Related articles</b></td>
      </tr>
      <tr>
      <td align="center">
        <input type="checkbox" name="fb_ent" value="relevant"/>Relevant 
        <input type="checkbox" name="fb_ent" value="irrelevant"/>Irrelevant 
        &nbsp;&nbsp;
        <input type="checkbox" name="fb_ent" value="enlightening"/>Enlightening
        <input type="checkbox" name="fb_ent" value="obvious"/>Obvious
        &nbsp;&nbsp;
        <input type="checkbox" name="fb_ent" value="accurate"/>Accurate
        <input type="checkbox" name="fb_ent" value="noisy"/>Noisy
        &nbsp;&nbsp;
        <input type="checkbox" name="fb_ent" value="concise"/>
        Concise
        <input type="checkbox" name="fb_ent" value="redundant"/>Redundant
      </td>
      <td align="center">
        <input type="checkbox" name="fb_prv" value="relevant"/>Relevant 
        <input type="checkbox" name="fb_prv" value="irrelevant"/>Irrelevant 
        &nbsp;&nbsp;
        <input type="checkbox" name="fb_prv" value="enlightening"/>Enlightening
        <input type="checkbox" name="fb_prv" value="obvious"/>Obvious
        &nbsp;&nbsp;
        <input type="checkbox" name="fb_prv" value="accurate"/>Accurate
        <input type="checkbox" name="fb_prv" value="noisy"/>Noisy
        &nbsp;&nbsp;
        <input type="checkbox" name="fb_prv" value="concise"/>
        Concise 
        <input type="checkbox" name="fb_prv" value="redundant"/>Redundant
      </td>
      </tr>
      <tr>
      <td align="center">
        Detailed comments:
        <br/>
        <textarea 
        style="font-family:'Open Sans Condensed',sans-serif;color:#8A8985;font-size:small" rows="5" cols="60" name="fb_ent_det"></textarea>
      </td>
      <td align="center">
        Detailed comments:
        <br/>
        <textarea 
        style="font-family:'Open Sans Condensed',sans-serif;color:#8A8985;font-size:small" rows="5" cols="60" name="fb_prv_det"></textarea>
      </td>
      </tr>
      </table>
      <input type="submit" value="Submit"/><br/>
      Not sure how to fill in the form? Consult a simple  
      <a href="/data/html/about.tmp#evaluation">guideline</a> then.
      </small>
    </form>
  </center>
