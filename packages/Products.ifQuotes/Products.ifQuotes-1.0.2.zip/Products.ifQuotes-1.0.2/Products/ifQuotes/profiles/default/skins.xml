<?xml version="1.0"?>

<object name="portal_skins"
        meta_type="Plone Skins Tool"
        allow_any="False"
        cookie_persistence="False"
        request_varname="plone_skin">

 <object name="ifquotes_templates"
         meta_type="Filesystem Directory View"
         directory="ifQuotes/skins/ifquotes_templates"/>
 <object name="ifquotes_styles"
         meta_type="Filesystem Directory View"
         directory="ifQuotes/skins/ifquotes_styles"/>
 <object name="ifquotes_images"
         meta_type="Filesystem Directory View"
         directory="ifQuotes/skins/ifquotes_images"/>
 
 <skin-path name="Plone Default">
  <layer insert-after="custom" 
         name="ifquotes_templates"/>
  <layer insert-after="custom" 
         name="ifquotes_styles"/>
  <layer insert-after="custom" 
         name="ifquotes_images"/>
 </skin-path>

</object>