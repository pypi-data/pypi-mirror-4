<div class="daviz-tab-view" tal:attributes="id view/tabname">
  <div ex:role="facet" ex:facetClass="TextSearch" ex:facetLabel="Live search"></div>
  <div class="daviz-views" ex:role="viewPanel">
  <tal:view define="latlng python:view.latlng">
  <div tal:define="lens python:view.lens"
    tal:condition="lens" tal:replace="structure lens" />
  <div ex:role="view"
    ex:viewClass="Map"
    tal:attributes="
      id python:view.__name__.replace('.', '-');
      ex:latlng string:.$latlng">
  </div>

  </tal:view>
  </div>
</div>
