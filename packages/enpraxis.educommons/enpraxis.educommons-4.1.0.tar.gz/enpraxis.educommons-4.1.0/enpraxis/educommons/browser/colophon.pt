<div id="portal-colophon"
     metal:define-macro="colophon"
     i18n:domain="plone">
  <metal:slot define-slot="colophon" />
  <div class="colophonWrapper">
    <ul id="portal-colophon-items">
      <li tal:condition="not:view/anonymous" 
	i18n:domain="eduCommons" 
	i18n:translate="">Welcome 
	<a href=""
	   tal:attributes="href view/homelink;"
	   tal:content="view/username"
           i18n:name="username">Bob</a></li>
      <li tal:repeat="action view/colophon_actions">
	<a href=""
	   tal:attributes="href action/url;"
	   tal:content="action/title">action title</a>
      </li>
    </ul>
    <tal:block
       tal:replace="structure string:<!-- eduCommons version: ${view/version} -->" />
  </div>
</div>
