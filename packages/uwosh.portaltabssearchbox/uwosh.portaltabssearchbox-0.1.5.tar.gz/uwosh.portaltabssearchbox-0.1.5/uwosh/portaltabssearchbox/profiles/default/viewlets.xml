<?xml version="1.0"?>
<!-- This file holds the setup configuration of the viewlet managers
     for the "uwosh.portaltabssearchbox" skin.
     -->

<object>

<!-- Sunburst Theme -->

 <hidden manager="plone.portalheader" skinname="Sunburst Theme">
  <viewlet name="plone.searchbox"/>
  <viewlet name="plone.global_sections"/>
 </hidden>

 <order manager="plone.portalheader" skinname="Sunburst Theme">
  <viewlet name="plone.skip_links"/>
  <viewlet name="plone.personal_bar"/>
  <viewlet name="plone.app.i18n.locales.languageselector"/>
  <viewlet name="plone.logo"/>
  <viewlet name="uwosh.global_sections"/>
 </order>

<!-- Responsive Theme -->

 <hidden manager="plone.portalheader" skinname="Responsive Theme">
  <viewlet name="plone.searchbox"/>
  <viewlet name="plone.global_sections"/>
 </hidden>

  <order manager="plone.portalheader" skinname="Responsive Theme">
  <viewlet name="plone.skip_links"/>
  <viewlet name="plone.personal_bar"/>
  <viewlet name="plone.app.i18n.locales.languageselector"/>
  <viewlet name="plone.logo"/>
  <viewlet name="uwosh.global_sections"/>
 </order>

</object>
