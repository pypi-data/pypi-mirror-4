#!/usr/bin/env python
# Copyright 2008, Holger Krekel
# we have to get to insert sys.path to find our own package
#
import os, sys
def main():
    d = os.path.dirname(os.path.dirname(os.path.dirname(
            os.path.realpath(sys.argv[0]))))
    #print "inserting", d
    sys.path.insert(0, d) 

    import vadm 
    vadm.cmdline.main() 

if __name__ == '__main__':
    main()

