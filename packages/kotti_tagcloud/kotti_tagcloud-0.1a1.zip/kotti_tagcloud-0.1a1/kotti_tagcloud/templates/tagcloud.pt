<div class='tagcloud' id="canvas-container">
    <canvas id="tags-canvas" />
    <div id="tags">
        <ul>
          <tal:repeat tal:repeat="tag tags">
            <li><a href="${api.url(api.root)}@@search-tag?tag=${tag.title}" class="tag">${tag.title}</a></li>
          </tal:repeat>
        </ul>
    </div>
</div>
