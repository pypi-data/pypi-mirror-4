<?xml version="1.0"?>
<object name="Press Kit"
   meta_type="Factory-based Type Information with dynamic views"
   i18n:domain="Products.pressKit" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <property name="title" i18n:translate="">Press Kit</property>
  <property name="description"
    i18n:translate="">Folder that holds press documents downloadable as a zip bundle.</property>
  <property name="content_meta_type">PressKit</property>
  <property name="content_icon">folder_icon.gif</property>
  <property name="product">Products.pressKit</property>
  <property name="factory">addPressKit</property>
  <property name="immediate_view">atct_edit</property>
  <property name="global_allow">True</property>
  <property name="filter_content_types">False</property>
  <property name="allow_discussion">True</property>
  <property name="default_view">presskit_view</property>
  <property name="view_methods">
    <element value="presskit_view" />
  </property>
  <alias from="(Default)" to="(dynamic view)" />
  <alias from="edit" to="atct_edit" />
  <alias from="sharing" to="@@sharing" />
  <alias from="view" to="(selected layout)" />
  <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${folder_url}/" visible="True">
    <permission value="View" />
  </action>
  <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
    <permission value="Modify portal content" />
  </action>
</object>
