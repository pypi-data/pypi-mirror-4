<div class="row">
  <div class="span9">
    <div class="page-header">
      <h2>${context.name}</h2>
    </div>

    <table class="table table-striped">
      <tbody>
        <tr tal:repeat="intr view.intrs">
          <td>
            ${structure: view.render_intr(intr)}
          </td>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="span3">
    <h3>Categories</h3>
    <ul>
      <li tal:repeat="category view.categories">
        <a href="${view.manage_url}/${category}/">
          ${category}
        </a>
      </li>
    </ul>
  </div>
</div>
