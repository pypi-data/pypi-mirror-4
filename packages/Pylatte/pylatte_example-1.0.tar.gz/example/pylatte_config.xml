<?xml version="1.0"?>


<config>
	<pylatte-server>
		<port>8000</port>
	</pylatte-server>

	<latteUrl>
		<pylatte>
			<pylatte-name>default</pylatte-name>
			<pylatte-pyl>index.html</pylatte-pyl>
		</pylatte>
		<pylatte-mapping>
			<pylatte-name>default</pylatte-name>
			<url-mapping>/</url-mapping>
		</pylatte-mapping>	
		<pylatte>
			<pylatte-name>index</pylatte-name>
			<pylatte-pyl>index.html</pylatte-pyl>
		</pylatte>
		<pylatte-mapping>
			<pylatte-name>index</pylatte-name>
			<url-mapping>/index</url-mapping>
		</pylatte-mapping>	
		<pylatte>
			<pylatte-name>favicon</pylatte-name>
			<pylatte-pyl>faviconUpload.html</pylatte-pyl>
		</pylatte>
		<pylatte-mapping>
			<pylatte-name>favicon</pylatte-name>
			<url-mapping>/faviconUpload</url-mapping>
		</pylatte-mapping>	
		
		<pylatte>
			<pylatte-name>demo1</pylatte-name>
			<pylatte-pyl>demo1.html</pylatte-pyl>
		</pylatte>
		<pylatte-mapping>
			<pylatte-name>demo1</pylatte-name>
			<url-mapping>/demo1</url-mapping>
		</pylatte-mapping>	
		<pylatte>
			<pylatte-name>demo2</pylatte-name>
			<pylatte-pyl>demo2.html</pylatte-pyl>
		</pylatte>
		<pylatte-mapping>
			<pylatte-name>demo2</pylatte-name>
			<url-mapping>/demo2</url-mapping>
		</pylatte-mapping>	
		<pylatte>
			<pylatte-name>demo3</pylatte-name>
			<pylatte-pyl>demo3.html</pylatte-pyl>
		</pylatte>
		<pylatte-mapping>
			<pylatte-name>demo3</pylatte-name>
			<url-mapping>/demo3</url-mapping>
		</pylatte-mapping>	
		<pylatte>
			<pylatte-name>demo4</pylatte-name>
			<pylatte-pyl>demo4.html</pylatte-pyl>
		</pylatte>
		<pylatte-mapping>
			<pylatte-name>demo4</pylatte-name>
			<url-mapping>/demo4</url-mapping>
		</pylatte-mapping>	
		
		
		<pylatte>
			<pylatte-name>sql</pylatte-name>
			<pylatte-pyl>sql.html</pylatte-pyl>
		</pylatte>
		<pylatte-mapping>
			<pylatte-name>sql</pylatte-name>
			<url-mapping>/sql</url-mapping>
		</pylatte-mapping>	
		
	</latteUrl>
	
	<latteFilter>
	
		<filter>
			<filter-name>head</filter-name>
			<filter-pyl>filter/head.html</filter-pyl>
		</filter>
		<filter-mapping>
			<filter-name>head</filter-name>
			<filter-url>index.html</filter-url>
			<filter-url>demo1.html</filter-url>
			<filter-url>demo2.html</filter-url>
			<filter-url>demo3.html</filter-url>
			<filter-url>demo4.html</filter-url>
		</filter-mapping>
	</latteFilter>
	
</config>


