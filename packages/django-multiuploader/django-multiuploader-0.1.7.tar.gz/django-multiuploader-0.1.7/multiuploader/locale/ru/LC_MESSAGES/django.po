#: multiuploader/forms.py:80 multiuploader/forms.py:88
msgid "File type is not supported"
msgstr "Тип файла не поддерживается"

#: multiuploader/forms.py:86
#, python-format
msgid ""
"Please keep filesize under %(max_file_size)s. Current file size %(filesize)s"
msgstr ""
"Размер файла должен быть меньше %(max_file_size)s, размер отправленного "
"файла составляет %(filesize)s"

#. Translators: Start and Stop uploading buttons captions
#: multiuploader/templates/multiuploader/form.html:18
msgid "Start"
msgstr "Начать загрузку"

#: multiuploader/templates/multiuploader/form.html:19
msgid "Stop"
msgstr "Остановить загрузку"

#. Translators: Errors and its descriptions
#: multiuploader/templates/multiuploader/form.html:22
msgid "Error"
msgstr "Ошибка"

#: multiuploader/templates/multiuploader/form.html:23
msgid "File is too big"
msgstr "Файл слишком большой"

#: multiuploader/templates/multiuploader/form.html:24
#: multiuploader/templates/multiuploader/form.html:29
msgid "File is too small"
msgstr "Файл слишком маленький"

#: multiuploader/templates/multiuploader/form.html:25
msgid "Filetype not allowed"
msgstr "Не разрешенный формат файла"

#: multiuploader/templates/multiuploader/form.html:26
msgid "Max number of files exceeded"
msgstr "Превышен лимит файлов"

#: multiuploader/templates/multiuploader/form.html:27
msgid "File exceeds maximum upload limit"
msgstr "Превышен максимальный объем для загрузки"

#: multiuploader/templates/multiuploader/form.html:28
msgid "File exceeds maximum size of upload"
msgstr "Файл превышает максимально установленный размер загрузки"

#: multiuploader/templates/multiuploader/form.html:30
msgid "File was only partially uploaded"
msgstr "Файл загружен частично"

#: multiuploader/templates/multiuploader/form.html:31
msgid "No File was uploaded"
msgstr "Файл не был загружен"

#: multiuploader/templates/multiuploader/form.html:32
msgid "Missing a temporary folder"
msgstr "Отсутствует временная папка"

#: multiuploader/templates/multiuploader/form.html:33
msgid "Failed to write file to disk"
msgstr "Ошибка записи на диск"

#: multiuploader/templates/multiuploader/form.html:34
msgid "File upload stopped by extension"
msgstr "Загрузка файла, была прервана расширением"

#: multiuploader/templates/multiuploader/form.html:35
msgid "Empty file upload result"
msgstr "Пустая загрузка"

#: multiuploader/templates/multiuploader/form.html:53
#: multiuploader/templates/multiuploader/noscript.html:5
msgid "Append files"
msgstr "Прикрепить"

#: multiuploader/templates/multiuploader/form.html:58
msgid "Delete files"
msgstr "Удалить"

#: multiuploader/templates/multiuploader/form.html:66
msgid ""
"You can choose multiple files at once, by holding Ctrl or Cmd (on Mac) "
"button, or just Drag'n'drop files here."
msgstr ""
"Вы можете выбрать несколько файлов сразу зажав клавишу Ctrl или Cmd (на "
"Mac), или просто перетащить файлы сюда"
