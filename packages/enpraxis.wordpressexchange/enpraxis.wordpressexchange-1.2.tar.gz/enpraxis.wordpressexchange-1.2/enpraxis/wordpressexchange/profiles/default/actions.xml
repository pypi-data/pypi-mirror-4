<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="import wordpress" meta_type="CMF Action" i18n:domain="wordpressexchange">
   <property name="title" i18n:translate="">WP Import</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:@@wpimport_form</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python: object.restrictedTraverse('plone_context_state').is_structural_folder() and not object.restrictedTraverse('plone_context_state').is_portal_root()</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="document_actions" meta_type="CMF Action Category">
  <object name="export wordpress" meta_type="CMF Action" i18n:domain="wordpressexchange">
   <property name="title" i18n:translate="">WP Export</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$object_url/@@wpexport_form</property>
   <property name="icon_expr">string:wordpress_icon.gif</property>
   <property name="available_expr">python:here.Type() == 'Page' or here.isPrincipiaFolderish</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
