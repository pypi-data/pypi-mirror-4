<?xml version="1.0"?>
<object>
    <order manager="plone.portalheader" skinname="INTK Theme" based-on="Sunburst Theme">
        <viewlet name="plonetheme.intk.head" insert-before="*" />
    </order>
    
    <order manager="plone.portalfooter" skinname="INTK Theme" based-on="Sunburst Theme">
        <viewlet name="plonetheme.intk.personal_bar" insert-before="*" />
        <viewlet name="plonetheme.intk.footer" insert-after="plonetheme.intk.personal_bar" />
    </order>
    
    <order manager="plonetheme.intk.headManager" skinname="INTK Theme">
        <viewlet name="plonetheme.intk.languageSelector" />
        <viewlet name="plonetheme.intk.logo" />
        <viewlet name="plonetheme.intk.menu" />
    </order>
    
    <order manager="plone.abovecontenttitle" skinname="INTK Theme">
       <viewlet name="plonetheme.intk.searchbox" insert-after="*" />
    </order>
    
    <order manager="plonetheme.intk.footerManager" skinname="INTK Theme">
       <viewlet name="plonetheme.intk.logoBottom" insert-after="*" />
    </order>
    
    <hidden manager="plonetheme.intk.headManager" skinname="INTK Theme">
        <viewlet name="plonetheme.intk.logo" />
        <viewlet name="plonetheme.intk.menu" />
    </hidden>
    
    <hidden manager="plone.portalheader" skinname="INTK Theme">
        <viewlet name="plone.skip_links"/>
        <viewlet name="plone.site_actions"/>
        <viewlet name="plone.personal_bar"/>
        <viewlet name="plone.searchbox"/>
        <viewlet name="plone.global_sections"/>
        <viewlet name="plone.logo"/>
        <viewlet name="plone.app.i18n.locales.languageselector"/>
   </hidden>
    
    <hidden manager="plone.abovecontent" skinname="INTK Theme">
        <viewlet name="plone.path_bar"/>
    </hidden>
    
    <hidden manager="plone.belowcontentbody" skinname="INTK Theme">
        <viewlet name="plone.belowcontentbody.relateditems"/>
    </hidden>
    
    <hidden manager="plone.belowcontent" skinname="INTK Theme">
        <viewlet name="plone.belowcontenttitle.keywords"/>
    </hidden>
    
    <hidden manager="plone.portalfooter" skinname="INTK Theme">
        <viewlet name="plone.footer"/>
        <viewlet name="plone.colophon"/>
        <viewlet name="plone.site_actions"/>
  </hidden>
    
</object>
