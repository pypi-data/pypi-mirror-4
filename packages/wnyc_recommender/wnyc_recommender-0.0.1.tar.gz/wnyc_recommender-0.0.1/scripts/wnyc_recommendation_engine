#!/usr/bin/python
from wnyc_recommender.server import run
import gflags
FLAGS = gflags.FLAGS
from sys import argv, exit, stderr

if __name__ == "__main__":
   try:
      arg = FLAGS(argv)[1:]
   except gflags.FlagsError, e:
      print >>stderr, "%s\\nUsage: %s ARGS\\n%s" % (e, argv[0], FLAGS)
      exit(1)
   run()

