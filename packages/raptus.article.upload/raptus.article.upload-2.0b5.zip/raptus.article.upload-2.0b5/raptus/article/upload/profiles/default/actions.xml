<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="upload" meta_type="CMF Action" i18n:domain="raptus.article">
   <property name="title" i18n:translate="">Upload</property>
   <property name="description" i18n:translate="">Batch upload files.</property>
   <property name="url_expr">string:${object_url}/@@upload</property>
   <property name="icon_expr"></property>
   <property name="available_expr">context/@@upload/uploadAvailable|nothing</property>
   <property name="permissions">
    <element value="Add portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
