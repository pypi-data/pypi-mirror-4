<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="nuplone"
      tal:omit-tag="">
  <fieldset tal:attributes="class string:concise ${view/@@dependencies}">
    <legend><tal:span replace="view/label">Widget label</tal:span> <sup tal:condition="view/required" class="required">*</sup></legend> <dfn class="infoPanel" i18n:attributes="title" title="Information" tal:define="description view/field/description" tal:condition="description" tal:content="description"/>
      <tal:field repeat="widget view/subform/widgets/values" replace="structure widget/render"/>
      <button type="button" class="remove jsOnly micro floatAfter">Remove</button>
  </fieldset>
</html>
