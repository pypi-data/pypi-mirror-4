<style type='text/css'>
	ul li {
		list-style-type: none !important;
		padding: 0px 0 !important;
	}
	#content {
		margin: 0px 0 !important;
	}
	input, textarea, select, .form-row p {
    	margin: 0px 0 !important;
    	padding: 0px 0 !important;
    }
    .dashboard #content {
   		width: auto !important;
	}
</style>


