<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plumi.content">

    <configlet
        title="Plumi settings"
        action_id="plumi.content.settings"
        appId="plumi.content"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@plumi-controlpanel"
        icon_expr="string:${portal_url}/spinner.gif"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>

