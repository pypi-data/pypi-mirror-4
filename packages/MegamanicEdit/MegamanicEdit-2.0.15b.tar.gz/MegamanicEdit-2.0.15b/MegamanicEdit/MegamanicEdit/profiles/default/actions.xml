<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object" meta_type="CMF Action Category">
     
  <object name="megamanic_edit" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">MegamanicEdit</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/megamanic_edit_self_and_children</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:object.isMegamanicEditable(object)</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>

  <object name="megamanic_add" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">MegamanicAdd</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/megamanic_add</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:object.isMegamanicEditableTemplateObject(object)</property>
   <property name="permissions">
    <element value="Add portal content"/>
   </property>
   <property name="visible">True</property>
  </object>

