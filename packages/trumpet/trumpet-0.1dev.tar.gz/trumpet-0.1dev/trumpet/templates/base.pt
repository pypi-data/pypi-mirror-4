<!doctype html>
<!--[if lt IE 7]><html class="no-js ie6" lang="en"><![endif]-->
<!--[if IE 7]><html class="no-js ie7" lang="en"><![endif]-->
<!--[if IE 8]><html class="no-js ie8" lang="en"><![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" dir="ltr" lang="en-US"><!--<![endif]-->
<head>
<title>${title}</title>
<link rel="shortcut icon" type="image/png" href="${request.static_url('trumpet:static/favicon.ico')}">
<meta http-equiv="Content-Type" content="text/html;charset=utf-8" >
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
       <script type="text/javascript" src="${request.static_url('deform:static/scripts/jquery-1.7.2.min.js')}"></script>
       <script type="text/javascript" src="${request.static_url('deform:static/scripts/jquery.form-3.09.js')}"></script>
       <script type="text/javascript" src="${request.static_url('deform:static/scripts/deform.js')}"></script>
       <script type="text/javascript" src="${request.static_url('deform:static/scripts/jquery-ui-1.8.11.custom.min.js')}"></script>
       <script type="text/javascript" src="${request.static_url('deform:static/scripts/jquery-ui-timepicker-addon.js')}"></script>
      <link rel="stylesheet" href="${request.static_url('trumpet:static/scss/stylesheets/screen.css')}" media="screen"/>
      <link rel="stylesheet" href="${request.static_url('deform:static/css/jquery-ui.css')}" media="screen"/>
      <link rel="stylesheet" href="${request.static_url('deform:static/css/jquery-ui-timepicker-addon.css')}" media="screen"/>
      <link rel="stylesheet" href="${request.static_url('deform:static/css/form.css')}" media="screen"/>
      <link rel="stylesheet" href="${request.static_url('deform:static/css/ui-lightness/jquery-ui-1.8.11.custom.css')}" media="screen"/>
</head>
<body>
<div class="page">
  <div id="top-bar">
    <div class="horizontal-menu">
      <div tal:replace="structure top_bar" />
    </div>
    <header class="header">
      <h1>${header}</h1>
      <h2>${subheader}</h2>
    </header>
    </div>
  <div class="two-col">
    <div class="sidebar">
      <div tal:replace="structure left_menu" />
    <span tal:condition="'logged_in' in request.session and request.session['logged_in']">
      <a href="${request.application_url}/logout">Logout ${request.session['logged_in']}</a>
    </span>
  </div>
    <div class="content">
      <div tal:replace="structure content" />
    </div>
  </div>
  <div class="footer">
  <div tal:replace="structure footer" />
  </div>
  <a href="${request.resource_url(request.context)}">root</a>
</div>
</body>
</html>
