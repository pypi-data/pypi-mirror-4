msgid "label_calTableHeaderBorderColor"
msgstr "Couleur de la bordure des cellules d'en-tête"

#. Default: 'Border width for TH tags'
msgid "label_calTableHeaderBorderWidth"
msgstr "Largeur de la bordure des cellules d'en-tête"

#. Default: 'Font color for TH tags'
msgid "label_calTableHeaderFontColor"
msgstr "Couleur de la police des cellules d'en-tête"

#. Default: 'Even row background color for TR tags'
msgid "label_calTableRowEvenBackgroundColor"
msgstr "Couleur de fond pour les lignes paires"

#. Default: 'Odd row background color for TR tags'
msgid "label_calTableRowOddBackgroundColor"
msgstr "Couleur de fond pour les lignes impaires"

# label "calendar help" on tab for the calendar help view
msgid "label_calendar_help"
msgstr "Aide de l'agenda"

#. Default: 'Continuing events box, background color'
msgid "label_continuingHeaderBackgroundColor"
