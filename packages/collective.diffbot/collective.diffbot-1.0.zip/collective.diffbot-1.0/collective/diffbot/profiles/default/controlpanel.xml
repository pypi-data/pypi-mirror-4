<?xml version="1.0"?>
<object
  name="portal_controlpanel"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="collective"
  purge="False">

  <configlet
    title="Diffbot API"
    action_id="collective.diffbot"
    appId="collective.diffbot"
    category="Products"
    condition_expr=""
    url_expr="string:${portal_url}/@@diffbot-controlpanel"
    icon_expr="string:${portal_url}/++resource++collective.diffbot.gif"
    visible="True"
    i18n:attributes="title">
      <permission>Manage portal</permission>
  </configlet>

</object>
