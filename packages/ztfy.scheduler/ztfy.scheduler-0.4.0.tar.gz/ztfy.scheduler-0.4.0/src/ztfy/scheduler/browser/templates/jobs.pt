<div class="form" i18n:domain="ztfy.scheduler">
	<h1>
		<span class="light">::</span>
		<span i18n:translate="">Running jobs</span>
		<span class="light">::</span>
	</h1>
	<fieldset>
		<legend i18n:translate="">List of currently running jobs</legend>
		<div tal:define="jobs view/jobs">
			<tal:loop repeat="job jobs">
				<tal:var content="job" />
				<br tal:condition="not:repeat/job/end" />
			</tal:loop>
		</div>
	</fieldset>
</div>