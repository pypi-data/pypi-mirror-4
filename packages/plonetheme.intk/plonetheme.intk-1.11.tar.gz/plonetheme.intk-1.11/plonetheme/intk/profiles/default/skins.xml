<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="INTK Theme">

 <object name="plonetheme_intk_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.intk:skins/plonetheme_intk_custom_images"/>
 <object name="plonetheme_intk_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.intk:skins/plonetheme_intk_custom_templates"/>
 <object name="plonetheme_intk_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.intk:skins/plonetheme_intk_styles"/>

 <skin-path name="INTK Theme" based-on="Sunburst Theme">
  <layer name="plonetheme_intk_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_intk_custom_templates"
     insert-after="plonetheme_intk_custom_images"/>
  <layer name="plonetheme_intk_styles"
     insert-after="plonetheme_intk_custom_templates"/>
 </skin-path>

</object>
