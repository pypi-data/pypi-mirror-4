<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">

<body>
<metal:column_one_slot fill-slot="column_one_slot">
</metal:column_one_slot>

<metal:main fill-slot="main">

<tal:block tal:define="redirect python:'Anonymous' in user.getRoles() and 'login' or '%s/Members/%s/createObject?type_name=Object'%(portal_url, user.getUserName())">
    <script language="javascript" tal:content="string: window.location = '${redirect}';">
    </script>
</tal:block>

</metal:main>

<metal:column_one_slot fill-slot="column_two_slot">
</metal:column_one_slot>

</body>
</html>
