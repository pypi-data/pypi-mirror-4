<div id="featured">
<dl class="portlet portletfeatured"
    i18n:domain="cs.featured"
    tal:define="destakatuak view/destakatuak">
  <dt class="portletHeader">
  </dt>

  <dd tal:repeat="destakatua destakatuak">
    <tal:define define="destakatua_obj destakatua/getObject">
    <a tal:attributes="href destakatua_obj/getLink"><img class="featuredimage" tal:condition="destakatua_obj/getImage" tal:attributes="src string:${destakatua/getURL}/image_tile"/><img  class="featuredimage" tal:condition="not: destakatua_obj/getImage" tal:attributes="src string:${here/portal_url}/featuredimage.gif" alt="" /></a>
    <a class="titulua" tal:content="destakatua/Title" tal:attributes="href destakatua_obj/getLink"></a>
    <span tal:content="structure destakatua_obj/getText"></span>
    
    </tal:define>
  </dd>
  <div class="visualClear"></div>
</dl>
</div>
