<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False">

 <object name="EasyAsPiIE"
    meta_type="Filesystem Directory View"
    directory="Products.EasyAsPiIE:skins/EasyAsPiIE"/>
 

 <skin-path name="*">
  <layer name="EasyAsPiIE"
     insert-before="custom"/>
<!-- need to insert before custom, yes before custom. This gets rid of the sandbox that is custom for files like IEFixes.css and iepngfix.htc.dtml -->
 </skin-path>

</object>
