<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.transcode">

    <configlet
        title="Transcode settings"
        action_id="collective.transcode.settings"
        appId="collective.transcode"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@transcode-controlpanel"
        icon_expr="string:${portal_url}/spinner.gif"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>

