<?xml version="1.0"?>
<!--*- mode: xml -*-->
<interface>
  <object class="GtkWindow" id="PersonRoleTypeStep">
    <property name="default_height">250</property>
    <property name="default_width">440</property>
    <child>
      <object class="GtkVBox" id="vbox1">
        <property name="border_width">5</property>
        <property name="spacing">5</property>
        <property name="visible">True</property>
        <child>
          <object class="ProxyLabel" id="person_role_label">
            <property name="data_type">unicode</property>
            <property name="label" translatable="yes">What kind of client are you adding ?</property>
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="expand">False</property>
          </packing>
        </child>
        <child>
          <object class="GtkTable" id="table1">
            <property name="n_columns">2</property>
            <property name="n_rows">2</property>
            <property name="row_spacing">5</property>
            <property name="visible">True</property>
            <child>
              <object class="ProxyLabel" id="kiwilabel2">
                <property name="data_type">unicode</property>
                <property name="visible">True</property>
              </object>
            </child>
            <child>
              <object class="ProxyLabel" id="kiwilabel3">
                <property name="data_type">unicode</property>
                <property name="visible">True</property>
              </object>
              <packing>
                <property name="bottom_attach">2</property>
                <property name="top_attach">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkRadioButton" id="individual_check">
                <property name="label" translatable="yes">A new individual</property>
                <property name="visible">True</property>
              </object>
              <packing>
                <property name="left_attach">1</property>
                <property name="right_attach">2</property>
              </packing>
            </child>
            <child>
              <object class="GtkRadioButton" id="company_check">
                <property name="active">False</property>
                <property name="group">individual_check</property>
                <property name="label" translatable="yes">A new company</property>
                <property name="visible">True</property>
              </object>
              <packing>
                <property name="bottom_attach">2</property>
                <property name="left_attach">1</property>
                <property name="right_attach">2</property>
                <property name="top_attach">1</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="padding">6</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="ProxyLabel" id="kiwilabel4">
            <property name="data_type">unicode</property>
            <property name="label" translatable="yes">What is their phone number ?</property>
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="position">2</property>
          </packing>
        </child>
        <child>
          <object class="GtkHBox" id="hbox2">
            <property name="visible">True</property>
            <child>
              <object class="ProxyLabel" id="kiwilabel5">
                <property name="data_type">unicode</property>
                <property name="visible">True</property>
              </object>
            </child>
            <child>
              <object class="ProxyEntry" id="phone_number">
                <property name="data_type">unicode</property>
                <property name="model_attribute">phone_number</property>
                <property name="visible">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="position">1</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="kiwilabel6">
                <property name="data_type">unicode</property>
                <property name="visible">True</property>
              </object>
              <packing>
                <property name="position">2</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="position">3</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
