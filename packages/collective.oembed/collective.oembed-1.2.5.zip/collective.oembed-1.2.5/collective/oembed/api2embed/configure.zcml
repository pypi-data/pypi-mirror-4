<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="collective.oembed">

  <utility factory=".twitteruser.TwitterUserAPI2Embed" name="twitteruser"/>

  <browser:page
    name="collective.oembed.api2embed"
    for="*"
    class=".api2embed_view.API2EmbedView"
    permission="zope2.View"
    template="api2embed_view.pt"
    />

</configure>