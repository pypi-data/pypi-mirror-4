<dl class="portlet portletGlobalNavigationPortlet"
    i18n:domain="collective.portlet.globalnav">

    <dt xclass="portletHeader" style="padding:0;">
        <span class="portletTopLeft"></span>
        <span class="portletTopRight"></span>
    </dt>

    <dd>
    <ul class="navTree">
    <tal:item  tal:repeat="tab view/items">
    <li class="portletItem" tal:attributes="id tab/id">
        <span><a href=""
               tal:content="tab/name"
               tal:attributes="href tab/url;
                               title tab/description|nothing;">
            Tab Name
            </a></span>
    </li>
    </tal:item>
    </ul>
    </dd>
</dl>
