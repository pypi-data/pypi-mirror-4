#!/usr/bin/python
#coding=utf-8

import sys
import os
from plublog.options import o

reload(sys)
sys.setdefaultencoding("utf-8")  # jinja2.Template.render() returns 'Unicode'

with open('index.html', 'w') as f:
    f.write(os.path.join('site', o.jinja2.get_template('index.html').render()))
    f.close()
