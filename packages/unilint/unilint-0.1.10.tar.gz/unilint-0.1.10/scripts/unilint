#!/usr/bin/env python

"""executable starter script for the unilint program"""

from __future__ import print_function
import sys
from unilint.unilint_main import unilint_main
from unilint.common import UnilintException

if __name__ == "__main__":
    try:
        sys.exit(unilint_main(sys.argv))
    except UnilintException as e:
        print(str(e), file=sys.stderr)
        sys.exit(1)
